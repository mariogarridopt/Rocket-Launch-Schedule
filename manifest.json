{
   "name": "Rocket Launch Schedule",
   "description": "List of all upcoming SpaceX Launches",
   "version": "18.1.31",
   "manifest_version": 2,

   "browser_action": {
      "default_icon": "img/icon_16.png",
      "default_popup": "popup.html"
   },
   "icons": {
      "16": "img/icon_16.png",
      "48": "img/icon_48.png",
      "128": "img/icon_128.png"
   },

   "permissions": [
      "https://launchlibrary.net/*"
   ]
}
